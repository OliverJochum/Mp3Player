<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<fx:root prefHeight="80.0" prefWidth="400.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<HBox spacing="20.0">
			<children>
				<ToggleButton fx:id="repeatButton" mnemonicParsing="false" text="Repeat" />
				<Button fx:id="skipBButton" mnemonicParsing="false" text="Skip B" />
				<ToggleButton fx:id="playpauseButton" mnemonicParsing="false" text="Play" />
				<Button fx:id="skipButton" mnemonicParsing="false" text="Skip" />
				<ToggleButton fx:id="shuffleButton" mnemonicParsing="false" text="Shuffle" />
			</children>
			<padding>
				<Insets left="20.0" right="20.0" top="20.0" />
			</padding>
		</HBox>
      <Slider layoutX="130.0" layoutY="58.0" fx:id="volumeSlider"/>
	</children>
	
</fx:root>
